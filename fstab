# /etc/fstab: static file system information.
# 
# Use 'blkid' to print the universally unique identifier for a
# device; this may be used with UUID= as a more robust way to name devices
# that works even if disks are added and removed. See fstab(5).
# 
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
# / was on /dev/sda9 during installation
UUID=4678d836-b788-4cd4-a7aa-0501e773ecac	/	ext4	errors=remount-ro	0	1
UUID=A01B-A2CF	/boot/efi	vfat	defaults	0	1 # /boot/efi was on /dev/sda2 during installation
UUID=f30d84bd-eead-4282-b2f6-aa209fc13ba0	none	swap	sw	0	0 # swap was on /dev/sda8 during installation
UUID=487821667821544A	/mnt/Windows	ntfs-3g	nosuid,nodev,nofail,x-gvfs-show,x-gvfs-name=Windows	0	0 # windows
UUID=4216518516517B3D	/mnt/Downloads	ntfs-3g	nosuid,nodev,nofail,x-gvfs-show,x-gvfs-name=Downloads	0	0 # downloads
UUID=B46A45826A4541FA	/mnt/Web	ntfs-3g	nosuid,nodev,nofail,x-gvfs-show,x-gvfs-name=Web	0	0 # web
UUID=3624D94C24D90FAB	/mnt/HazeDisk	ntfs-3g	nosuid,nodev,nofail,x-gvfs-show,x-gvfs-name=HazeDisk	0	0 # HazeDisk
UUID=1604-AB62	/media/usbstick	vfat	defaults	0	1
